{# src/MA/MainBundle/Resources/Main/indexMain.html.twig #}

{% extends "MAMainBundle::main_general.html.twig" %}

{% block title %}
  {{ parent() }}
{% endblock %}

{% block MA_Main_body %}

	<!-- BEGIN CONTENT -->
	<div id="page_content" class="row">

		<div id="sessionFlash" class="sessionFlash">
		</div>

		<div id="fullpage">

			<!-- Thank you -->
			<div class="section center" id="thankYou">
				<div class="row center">
		        		<div class="col-lg-2 col-md-2 col-sm-2 hidden-xs"></div>
		        		<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
							<h2>
								{{ 'contact.titles.thankYou' | trans }}
