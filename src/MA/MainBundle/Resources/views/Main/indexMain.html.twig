				{% for inc in 1..37 %}
					{% set photoLink = 'img/photos/' ~ inc ~ '.jpg' %}
					<script>
                  		var imgLink = "{{ asset(photoLink)}}";
                  		listImages['{{ inc }}'] = imgLink;
					</script>
				{% endfor %}
				
				<script type="text/javascript">
                  	$(function(){
                  		// Set photobackground to window size
                  		var window_height = window.innerHeight;
					    var window_width = window.innerWidth;
					    $('.photo_background').css({'height':window_height+'px', 'width':window_width+'px'});

					    // Set photos square
                  		var bg_photo_height = $('.photo_background_sq').width();
	                  	var bg_photo_height = $('.photo_background_sq').width();
	                  	$('.photo_background_sq').css({'height':bg_photo_height+'px'});
	                  	var bg_photo_content_height = $('.photo_background_sq_content').width();
	                  	$('.photo_background_sq_content').css({'height':bg_photo_content_height+'px'});

	                  	
	                  	// Div ids
	                  	var listIds = [];
	                  	var listActive = [];
	                  	for (var i = 0; i < 34; i++) {
	                  		var el = '#photo_background_sq_'+i;
	                  		var face = '#photo_background_sq_'+i+'_f';
	                  		listIds[i] = el;
		                  	$(el).flip();
	                  		listActive[i] = true;
	                  		// Images displayed
	                  		var imgToDisplay = listImages[Math.floor(Math.random()*listImages.length)];
	                  		$(face).css({'background-image':'url('+imgToDisplay+')'});
	                  	}

	                  	// Axis
	                  	var axisList = ['x', 'y'];

	                  	// Set flip random
	                  	setInterval(function () {
	                  		var rank = Math.floor(Math.random()*listIds.length);
	                  		var idToFlip = listIds[rank];
	                  		var isActive = listActive[rank];
	                  		if (isActive == true) {
	                  			// Change back
	                  			var face = idToFlip+'_b';
	                  			var imgToDisplay = listImages[Math.floor(Math.random()*listImages.length)];
	                  			$(face).css({'background-image':'url('+imgToDisplay+')'});
	                  			listActive[rank] = false;
	                  		}
	                  		else {
	                  			// Change front
	                  			var face = idToFlip+'_f';
	                  			var imgToDisplay = listImages[Math.floor(Math.random()*listImages.length)];
	                  			$(face).css({'background-image':'url('+imgToDisplay+')'});	
	                  			listActive[rank] = true;
	                  		}
	                  		var axisSelect = axisList[Math.floor(Math.random()*axisList.length)];
	                  		$(idToFlip).flip(isActive);
	                  		// {
	                  		// 	reverse: isActive,
		                  	// 	axis: axisSelect,
		                  	// 	speed: 1000
	                  		// });
	                  	}, 1000);
				    });
